{
  "name": "hightlight-errors",
  "displayName": "hightlight-errors",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hightlight-errors.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "highlight-errors.executeCodeAction",
        "title": "Print Code Actions"
      },
      {
        "command": "highlight-errors.goToReferences",
        "title": "My Go to References"
      },
      {
        "command": "highlight-errors.smartGoto",
        "title": "Smart Goto Definition/References"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "prettier": "^3.5.2"
  }
}
